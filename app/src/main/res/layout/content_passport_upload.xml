<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="@dimen/common_spacing_big"
    android:layout_marginLeft="@dimen/common_spacing_big"
    android:layout_marginRight="@dimen/common_spacing_big"
    android:background="@color/colorLightGray"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.purplelight.redstar.PassportUploadActivity"
    tools:showIn="@layout/app_bar_passport_upload">

    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/common_spacing_big"
        android:background="@color/colorLightGray"
        android:dividerHeight="@dimen/common_spacing_big"
        android:divider="@null">
    </ListView>

</RelativeLayout>